public class TeamDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		List teams = session.createQuery("from Team t WHERE"
				+ "t.teamName = ? AND t.league = 'NHL'")
				.setString(0, "Chicago Blackhawks").list();

	}

}
